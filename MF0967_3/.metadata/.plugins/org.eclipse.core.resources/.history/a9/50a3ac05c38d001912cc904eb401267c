package com.ipartek.formacion.uf2216.ejemplos.poo;

public class Punto {
	private int x, y;

	public int getX() {
		return x;
	}

	public void setX(int x) {
		if(x > 100 || x < -100) {
			System.out.println("El dato no es válido");
		} else
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}
}
