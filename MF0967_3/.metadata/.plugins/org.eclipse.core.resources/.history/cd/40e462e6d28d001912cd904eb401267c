package com.ipartek.formacion.uf2216.ejemplos.poo;

public class PoligonoPrueba {

	public static void main(String[] args) {
		Poligono p = new Poligono();
		
		p.add(new Punto(1,2));
		p.add(new Punto(3,4));
		
		Iterable<Punto> i = p.getPuntos();
		
		for(Punto pt: i) {
			System.out.println(pt.getXY());
		}
	}

}
